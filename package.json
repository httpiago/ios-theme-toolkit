{
  "name": "ios-theme-toolkit",
  "version": "0.4.0",
  "description": "A simple package of components in React that follow the style guide of iOS for you to use in your web apps.",
  "author": "Iago bruno <httpiago@gmail.com> (https://www.iagobruno.com)",
  "repository": "github:httpiago/ios-theme-toolkit",
  "homepage": "https://httpiago.github.io/ios-theme-toolkit/",
  "bugs": "https://github.com/httpiago/ios-theme-toolkit/issues",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "create-component": "node ./scripts/create-component.js",
    "dev": "gulp watch",
    "build": "gulp build",
    "docz": "docz dev",
    "deploy-docs": "docz build && gh-pages -d .docz/dist",
    "publish": "yarn run build && yarn publish && yarn run deploy-docs",
    "prepush": "yarn run deploy"
  },
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "devDependencies": {
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "colors": "^1.3.2",
    "cross-env": "^5.2.0",
    "dedent": "^0.7.0",
    "docz": "^0.12.12",
    "docz-plugin-css": "^0.11.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.0",
    "fs-extra": "^7.0.1",
    "gh-pages": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.10.0",
    "gulp-concat": "^2.6.1",
    "gulp-less": "^4.0.1",
    "gulp-plumber": "^1.2.0",
    "gulp-rename": "^1.4.0",
    "inquirer": "^6.2.0",
    "ios-theme-toolkit": "file:./",
    "prepush": "^3.1.11",
    "rollup": "^0.67.3",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "template-file": "^3.0.1"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "prepush": {
    "tasks": [
      "yarn run prepush"
    ]
  },
  "keywords": [ "react", "ui-kit", "react-components", "component", "css", "frontend", "ios" ],
  "license": "MIT"
}
