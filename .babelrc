{
  "env": {
    "build": {
      "presets": [
        ["@babel/preset-env", {
          "modules": false
        }],
        "@babel/preset-react"
      ],
      "exclude": "node_modules/**",
      "plugins": [ "@babel/plugin-external-helpers", "@babel/plugin-proposal-class-properties" ]
    },
    "test": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
      ]
    }
  }
}
